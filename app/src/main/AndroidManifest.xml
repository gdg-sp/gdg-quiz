<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.org.gdgsp.gdgquiz.view" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="br.org.gdgsp.gdgquiz.view.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="br.org.gdgsp.gdgquiz.view.QuestionActivity"
            android:label="@string/title_activity_pergunta" >
        </activity>
        <activity
            android:name="br.org.gdgsp.gdgquiz.view.ResultActivity"
            android:label="@string/title_activity_result" >
        </activity>
    </application>

</manifest>
